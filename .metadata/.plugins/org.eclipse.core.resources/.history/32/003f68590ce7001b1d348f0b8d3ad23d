package companyDetails;

import java.util.Scanner;

public class MyClass 
{
	static int factorial(int n)
	{
		int i,fact=1;
		for(i=1;i<=n;i++)
			fact=fact*i;
		return fact;
	}
	
	
	public static void main(String[] args) 
	{
		
		int n1=4,n2=5,n3=7;
		
		System.out.println(factorial(n1));
		System.out.println(factorial(n2));
	
	}
}
